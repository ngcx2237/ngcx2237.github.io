---
import PostMeta from '~/components/PostMeta.astro'
import FootnoteNavigation from '~/components/FootnoteNavigation.astro'
import type { Post } from '~/types'

interface Props {
  post: Post
}

const { post } = Astro.props
---

<article class="prose">
  <PostMeta post={post} />
  <slot />
  <FootnoteNavigation />
</article>
